.bg {
  width: 100%;
  height: 100%;
  background-color: rgb(243, 243, 243);
}

.bg .cu-form-group {
  display: flex;

  min-height: 100rpx;
  padding: 1rpx 30rpx;
  background-color: white;
  border-bottom: rgb(243, 243, 243) 1px solid;
  justify-content: space-between;
  align-items: center;
}

.bg .cu-form-group .title {
  position: relative;

  height: 60rpx;
  padding-right: 30rpx;

  text-align: justify;
  font-size: 30rpx;
  line-height: 60rpx;
}

.bg .cu-form-group picker {
  position: relative;
  padding-right: 40rpx;
  flex: 1;
  overflow: hidden;
}

.bg .cu-form-group picker .picker {
  width: 100%;

  text-align: right;
  font-size: 28rpx;
  line-height: 100rpx;
  white-space: nowrap;
  overflow: hidden;

  text-overflow: ellipsis;
}

.bg .btn {
  margin-top: 20px;
  background-color: white;
}
.priceTable{
  margin-top: 20rpx;
}
.priceTable .line {
  display: flex;
  justify-content: space-around;
  min-height: 100rpx;
  padding: 1rpx 30rpx;
  width: 100%;
  background: white;
  align-items: center;
}

.priceTable .line .part1 {
  display: flex;
  width: 33.3%;
  align-items: center;
}

.priceTable .line .part2 {
  width: 33.3%;
  align-items: center;
  display: flex;
  justify-content: flex-end;
}

.priceTable .line .part3 {
  width: 33.3%;
  align-items: center;
  display: flex;
  justify-content: center;
  border-bottom: rgb(243, 243, 243) 1px solid;
}
.delIcon{
  width:50rpx;
  height:50rpx;
}