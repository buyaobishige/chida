<!-- 新的：接口对其了H5 -->
<canvas
  wx:if="{{isUseNewCanvas}}"
  class="ec-canvas"
  canvas-id="{{ canvasId }}"
  type="2d"
  bindinit="init"
  bindtouchstart="{{ ec.disableTouch ? '' : 'touchStart' }}"
  bindtouchmove="{{ ec.disableTouch ? '' : 'touchMove' }}"
  bindtouchend="{{ ec.disableTouch ? '' : 'touchEnd' }}"
/>
<!-- 旧的 -->
<canvas
  wx:else
  class="ec-canvas"
  canvas-id="{{ canvasId }}"
  bindinit="init"
  bindtouchstart="{{ ec.disableTouch ? '' : 'touchStart' }}"
  bindtouchmove="{{ ec.disableTouch ? '' : 'touchMove' }}"
  bindtouchend="{{ ec.disableTouch ? '' : 'touchEnd' }}"
/>
